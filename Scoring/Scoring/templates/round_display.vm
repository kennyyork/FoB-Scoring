<html>
</head>
	$css	
</head>
<body>
<div class="mainHeader" id="roundDisplayHeader"><p>$title</p></div>
#foreach($round in $rounds)
#beforeall
<div class="roundDisplay">
	<table id="roundDislayTable">
		<thead>
			<tr><th>Number</th><th>Red</th><th>Green</th><th>Blue</th><th>Yellow</th></tr>
		</thead>
		<tbody>
#afterall
		</tbody>		
	</table>
</div>	
#each
			<tr><td>$round.Number</td><td class="redCell">$round.Red.Name</td><td class="greenCell">$round.Green.Name</td><td class="blueCell">$round.Blue.Name</td><td class="yellowCell">$round.Yellow.Name</td></tr>
#end
</body>
</html>